{
  "entryFile": "src/didweb/index.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["src/didweb/*Controller.ts"],
  "spec": {
    "outputDirectory": "build/didweb/routes",
    "specVersion": 3
  },
  "routes": {
    "routesDir": "src/didweb/routes",
    "iocModule": "./src/ioc",
    "esm": true,
    "noWriteIfUnchanged": true
  }
}