{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>digicatapult/renovate-config"],
  "packageRules": [
    {
      "matchPackagePatterns": ["^@credo-ts/"],
      "enabled": false
    },
    {
      "matchPackageNames": ["@2060.io/credo-ts-didcomm-media-sharing"],
      "enabled": false
    },
    {
      "matchPackageNames": ["@hyperledger/anoncreds-nodejs"],
      "enabled": false
    },
    {
      "matchPackageNames": ["@swc/core"],
      "enabled": false
    }
  ]
}
